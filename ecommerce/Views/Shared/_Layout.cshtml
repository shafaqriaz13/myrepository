    <div class="header_section">
        <div class="container-fluid">
            <nav class="navbar">
                <div id="mySidenav" class="sidenav">
                    <a href="javascript:void(0)" class="closebtn" onclick="closeNav()">&times;</a>
                    <ul>
                        
                         
                        <li><a asp-controller="Home" asp-action="Index">Home</a>></li>
                        <li><a asp-controller="Home" asp-action="Products">Products</a>></li>
                        <li><a asp-controller="Home" asp-action="About">About</a>></li>
                        <li><a asp-controller="Home" asp-action="Client">Client</a></li>
                        <li><a asp-controller="Home" asp-action="Contact">Contact</a></li>
                    </ul>
                </div>
                <span class="toggle_icon" onclick="openNav()"><img src="~/NewFolder/images/toggle-icon.png" alt="Toggle Icon"></span>
                <a class="logo" href="index.html"><img src="~/NewFolder/images/logo.png" alt="Logo"></a>
                <form class="form-inline">
                    <div class="login_text">
                        <ul>
                            <li><a href="#"><img src="~/NewFolder/images/user-icon.png" alt="User Icon"></a></li>
                            <li><a href="#"><img src="~/NewFolder/images/bag-icon.png" alt="Bag Icon"></a></li>
                            <li><a href="#"><img src="~/NewFolder/images/search-icon.png" alt="Search Icon"></a></li>
                        </ul>
                    </div>
                </form>
            </nav>
        </div>
    </div>

    <script>
        function openNav() {
            document.getElementById("mySidenav").style.width = "250px";
        }

        function closeNav() {
            document.getElementById("mySidenav").style.width = "0";
        }
    </script>
</body>
</html>


   
    
    

